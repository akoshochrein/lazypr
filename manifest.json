{
    "name": "LazyPR",
    "description": "Templates for your pull requests!",
    "version": "0.0.1",
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "icons": {
        "128": "img/icon128.png"
    },
    "content_scripts": [{
        "matches": [
            "https://github.com/*/*/compare/*",
            "http://github.com/*/*/compare/*"
        ],
        "js": ["inline_content.js"],
        "run_at": "document_end"
    }],
    "manifest_version": 2
}
