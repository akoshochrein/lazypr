{
    "name": "LazyPR",
    "description": "Templates for your pull requests!",
    "version": "0.0.1",
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "icons": {
        "128": "img/icon128.png"
    },
    "browser_action": {
        "default_title": "Inset Pull Request Template",
        "default_icon": "img/icon38.png",
        "default_popup": "index.html"
    },
    "manifest_version": 2
}
